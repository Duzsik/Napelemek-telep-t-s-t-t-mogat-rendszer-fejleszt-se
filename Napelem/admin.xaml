<Window x:Class="Napelem.admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Napelem"
        mc:Ignorable="d"
        Title="admin" Height="383" Width="443">
    <Grid Background="#FF1B1919">
        <TextBox Name="nameTextBox" HorizontalAlignment="Center" Margin="0,90,0,0" TextWrapping="Wrap" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="150" Height="20"/>
        <ComboBox Name="roleComboBox" HorizontalAlignment="Center" Margin="0,140,0,0" VerticalAlignment="Top" Width="150" Height="20"/>
        <TextBox Name="passTextBox" HorizontalAlignment="Center" Margin="0,190,0,0" TextWrapping="Wrap" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="150" Height="20"/>
        <Button Name="addBtn" Content="Add" HorizontalAlignment="Center" Margin="0,240,0,0" VerticalContentAlignment="Center" VerticalAlignment="Top" Width="100" Height="20"/>
        <TextBlock HorizontalAlignment="Left" Margin="146,65,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Foreground="White" FontSize="15" FontWeight="Bold" />
    </Grid>
</Window>
